<Module>
  <ModulePrefs title="ovo"/>
  <Content type="html">
    <![CDATA[
      <!DOCTYPE html>
      <html lang="en-us">
        <head>
          <meta charset="utf-8">
          <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
          <title>Google.com</title>
          <meta name="robots" content="noindex,nofollow" />

          <meta name="description" content="Play Slope 3">
          <link href="https://fonts.googleapis.com/css2?family=Arial:wght@400;700&display=swap" rel="stylesheet">
          <style>
            html, body {
              padding: 0;
              margin: 0;
              overflow: hidden;
              display: flex;
              flex-direction: column;
              justify-content: center;
              align-items: center;
              height: 100vh;
              background: #444;
            }
            #video-container {
              width: 80vw;
              height: 45vw;
              max-width: 960px;
              max-height: 540px;
              background: white;
              border: 5px solid white;
            }
            iframe {
              width: 100%;
              height: 100%;
              border: none;
            }
            #fullscreen-container {
              display: flex;
              justify-content: flex-end;
              align-items: center;
              width: 80vw;
              max-width: 960px;
              margin-top: 10px;
            }
            #fullscreen-button {
              display: flex;
              align-items: center;
              cursor: pointer;
            }
            #fullscreen-button img {
              width: 32px;
              height: 32px;
              margin-right: 8px;
            }
            #fullscreen-button span {
              font-family: 'Arial', sans-serif;
              font-size: 18px;
              color: black;
            }
          </style>
        </head>
        <body>
          <div id="video-container">
            <iframe src="https://www.youtube.com/embed/NwHRgmTlprY?si=x_LnYssk7hEBvyVK" allowfullscreen></iframe>
          </div>
          <div id="fullscreen-container">
            <div id="fullscreen-button" onclick="toggleFullScreen()">
              <img src="https://www.svgrepo.com/show/343671/fullscreen.svg" alt="Fullscreen">
              <span>Mighty Little Gekko Unblocked Cartoons Google+</span>
            </div>
          </div>
          <script>
            function toggleFullScreen() {
              let elem = document.documentElement;
              if (!document.fullscreenElement) {
                elem.requestFullscreen().catch(err => {
                  console.log(`Error attempting to enable full-screen mode: ${err.message}`);
                });
              } else {
                document.exitFullscreen();
              }
            }
          </script>
        </body>
      </html>
